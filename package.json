{
  "name": "@optune/react-big-calendar",
  "version": "0.20.0",
  "description": "Optune Calendar as copy of the awesome react-big-calendar Version 0.20",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "contributors": [
    {
      "name": "Joel Meiller",
      "email": "joel.meiller@gmail.com"
    }
  ],
  "repository": "optune/react-big-calendar",
  "license": "MIT",
  "main": "lib/index.js",
  "style": "lib/css/react-big-calendar.css",
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "calendar",
    "events",
    "full calendar",
    "optune"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "clean:examples": "rimraf examples/static",
    "l": "lessc --autoprefix=\"ie >= 10, safari >= 8, last 2 versions\" ",
    "less": "yarn l src/less/styles.less ./lib/css/react-big-calendar.css && yarn less-dnd",
    "less-dnd": "yarn l src/addons/dragAndDrop/styles.less ./lib/addons/dragAndDrop/styles.css",
    "assets": "cpy src/less/* lib/less && yarn assets-addons",
    "assets-addons": "cpy addons/**/*.less ../lib/ --cwd=src --parents",
    "build": "babel -d lib/ src/ && yarn assets && yarn less",
    "build:examples": "yarn clean:examples && webpack --config examples/webpack.config.js",
    "build:prod": "yarn clean && babel -d lib/ src/ && yarn assets && yarn less",
    "watch": "yarn build:dev -w",
    "examples": "yarn clean:examples && webpack-dev-server --config examples/webpack.config.js --mode development",
    "lint": "eslint --ext js,jsx src/ test/",
    "storybook": "start-storybook -p 9002",
    "test": "yarn lint && jest",
    "tdd": "jest --watch",
    "release": "rollout",
    "prepublishOnly": "yarn build",
    "prettier": "prettier '**/*js' !examples/bundle.js '!lib/**' --write ",
    "prepush": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "peerDependencies": {
    "moment": "^2.24.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@4c/rollout": "^1.3.3",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "acorn": "^6.1.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-preset-jason": "^6.0.1",
    "bootstrap": "^4.3.1",
    "component-metadata-loader": "^4.0.0",
    "cpy-cli": "^2.0.0",
    "eslint": "^5.15.1",
    "eslint-config-jason": "^5.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "font-awesome": "^4.7.0",
    "globalize": "^1.4.2",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jquery": "^3.3.1",
    "less": "^3.9.0",
    "less-plugin-autoprefix": "^2.0.0",
    "lint-staged": "^8.1.5",
    "markdown-jsx-loader": "^3.0.2",
    "marked": "^0.6.1",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "^1.16.4",
    "react-bootstrap": "^0.32.4",
    "react-docgen": "^4.1.0",
    "react-tackle-box": "^2.1.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.19.1",
    "webpack-atoms": "^8.3.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "date-arithmetic": "^3.1.0",
    "dom-helpers": "^3.4.0",
    "invariant": "^2.2.4",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.0",
    "prop-types": "^15.7.2",
    "prop-types-extra": "^1.1.0",
    "react-overlays": "^1.2.0",
    "uncontrollable": "^6.1.0",
    "warning": "^4.0.3"
  }
}
